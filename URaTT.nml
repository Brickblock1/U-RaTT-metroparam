	template_object (70)
}

spritelayout switch_URaTT_Urban_Objects {
    ground {
        sprite: URaTT_Urban_Objects_ground;
	}
	building {
        sprite: URaTT_Urban_Objects_ground;
		hide_sprite: (terrain_type == TILETYPE_SNOW);
    }
	building {
        sprite: URaTT_Urban_Objects_ground_snow;
		hide_sprite: (terrain_type != TILETYPE_SNOW);
    }
}
item (FEAT_OBJECTS, item_URaTT_Urban_Objects) {
    property {
        class:                  		"URTO";
        classname:              		string(STR_URTO);
        name:                   		string(STR_URaTT_Urban_Objects);
        climates_available:     		ALL_CLIMATES;
        size:                   		[1,1];
        build_cost_multiplier:  		5;
        remove_cost_multiplier: 		10;
        introduction_date:      		date(1912,1,1);
        end_of_life_date:       		0xFFFFFFFF;
        object_flags:          			bitmask(OBJ_FLAG_ALLOW_BRIDGE, OBJ_FLAG_ANYTHING_REMOVE, OBJ_FLAG_ONLY_INGAME);
        height:                			0;
    }
    graphics {
	default:							switch_URaTT_Urban_Objects;
