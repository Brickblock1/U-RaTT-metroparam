grf {
	grfid: "UFI\89";
	name: string(STR_GRF_NAME);
	desc: string(STR_GRF_DESCRIPTION);
	url:  string(STR_GRF_URL);
	version: 130;
	min_compatible_version: 120;
			param 0 {
        param_gui {
            type: bool;
            name: string(STR_PARAM_GUI_NAME);
            desc: string(STR_PARAM_GUI_DESC);
            def_value: 0;
				}
			}
		    param 1 {
        param_road_available {
            type: int;
            name: string(STR_PARAM_ROAD_AVAILABLE_NAME);
            desc: string(STR_PARAM_ROAD_AVAILABLE_DESC);
			min_value: 0;
			max_value: 4;
            def_value: 0;
            names: {
					0: string(STR_PARAM_ROAD_AVAILABLE_0);
                    1: string(STR_PARAM_ROAD_AVAILABLE_1);
					2: string(STR_PARAM_ROAD_AVAILABLE_2);
					3: string(STR_PARAM_ROAD_AVAILABLE_3);
					4: string(STR_PARAM_ROAD_AVAILABLE_4);
                    };
				}
			}
			param 2 {
        param_ardx_speed {
            type: int;
            name: string(STR_PARAM_ARDX_SPEED_NAME);
            desc: string(STR_PARAM_ARDX_SPEED_DESC);
            min_value: 0;
            max_value: 3;
            def_value: 1;
            names: {
					0: string(STR_PARAM_ARDX_SPEED_SLOW);
					1: string(STR_PARAM_ARDX_SPEED_DEFAULT);
                    2: string(STR_PARAM_ARDX_SPEED_FAST);
					3: string(STR_PARAM_ARDX_SPEED_NOLIMIT);
                    };
				}
			}
			param 3 {
        param_urdx_speed {
            type: int;
            name: string(STR_PARAM_URDX_SPEED_NAME);
            desc: string(STR_PARAM_URDX_SPEED_DESC);
            min_value: 0;
            max_value: 3;
            def_value: 1;
            names: {
					0: string(STR_PARAM_URDX_SPEED_SLOW);
					1: string(STR_PARAM_URDX_SPEED_DEFAULT);
                    2: string(STR_PARAM_URDX_SPEED_FAST);
					3: string(STR_PARAM_URDX_SPEED_NOLIMIT);
                    };
				}
			}
			param 4 {
        param_irdx_speed {
            type: int;
            name: string(STR_PARAM_IRDX_SPEED_NAME);
            desc: string(STR_PARAM_IRDX_SPEED_DESC);
            min_value: 0;
            max_value: 3;
            def_value: 1;
            names: {
					0: string(STR_PARAM_IRDX_SPEED_SLOW);
					1: string(STR_PARAM_IRDX_SPEED_DEFAULT);
                    2: string(STR_PARAM_IRDX_SPEED_FAST);
					3: string(STR_PARAM_IRDX_SPEED_NOLIMIT);
                    };
				}
			}
		    param 5 {
        param_build {
            type: int;
            name: string(STR_PARAM_BUILD_NAME);
            desc: string(STR_PARAM_BUILD_DESC);
            min_value: 0;
            max_value: 2;
            def_value: 0;
            names: {
					0: string(STR_PARAM_BUILD_NORMAL);
                    1: string(STR_PARAM_BUILD_HARD);
					2: string(STR_PARAM_BUILD_VERY_HARD);
                    };
				}
			}
		    param 6 {
        param_maintenance {
            type: int;
            name: string(STR_PARAM_MAINTENANCE_NAME);
            desc: string(STR_PARAM_MAINTENANCE_DESC);
            min_value: 0;
            max_value: 2;
            def_value: 0;
            names: {
					0: string(STR_PARAM_MAINTENANCE_NORMAL);
                    1: string(STR_PARAM_MAINTENANCE_HARD);
					2: string(STR_PARAM_MAINTENANCE_VERY_HARD);
                    };
				}
			}
			param 7 {
        param_style {
            type: bool;
            name: string(STR_PARAM_STYLE_NAME);
            desc: string(STR_PARAM_STYLE_DESC);
            def_value: 0;
				}
			}
		    param 8 {
        param_road_one_way {
            type: int;
            name: string(STR_PARAM_ONE_WAY_NAME);
            desc: string(STR_PARAM_ONE_WAY_DESC);
            min_value: 0;
            max_value: 1;
            def_value: 0;
            names: {
					0: string(STR_PARAM_ONE_WAY_0);
                    1: string(STR_PARAM_ONE_WAY_1);
                    };
				}
			}
			param 9 {
		param_roadstop {
			type: bool;
			name: string(STR_PARAM_ROADSTOP_NAME);
			desc: string(STR_PARAM_ROADSTOP_DESC);
			def_value: 0;
			}
		}
			param 10 {
		param_roadstop_tile {
			type: bool;
			def_value: 0;
			name: string(STR_PARAM_ROADSTOP_TILE_NAME);
			desc: string(STR_PARAM_ROADSTOP_TILE_DESC);
			}
		}
	}

	if (ttd_platform != PLATFORM_OPENTTD || (openttd_version < version_openttd(1, 10, 0, 0))) {
		error(FATAL, REQUIRES_OPENTTD, string(STR_OPENTTD_VERSION));
	}


//==================================================================================================================================
//								PARAMETR
//==================================================================================================================================
	if (param_build == 1) {
		basecost {
			PR_BUILD_ROAD: 1;
			}
		} else
	if (param_build == 2) {
		basecost {
			PR_BUILD_ROAD: 2;
			}
		}

	if (param_maintenance == 0) {
		basecost {
			PR_MAINTENANCE_ROAD: -1;
			}
		} else
	if (param_maintenance == 2) {
		basecost {
			PR_MAINTENANCE_ROAD: 1;
			}
		}

