											 "ARD0", "ARD1", "ARD2", "ARD3", "ARD4", "ARD5", "WRD0", "WRD1", "URD0", "URD1", "IRD0", "IRD1",
											"ELRD", "AER0", "AER1", "AER2", "UER0", "UER1"];
			alternative_roadtype_list:		["ROAD", "ORD0", "ORD1", "ORD2", "ORD3", "ORD4", "SRD0", "SRD1", "SRD2",
											 "ARD0", "ARD1", "ARD2", "ARD3", "ARD4", "ARD5", "WRD0", "WRD1", "URD0", "URD1", "IRD0", "IRD1",
											"ELRD", "AER0", "AER1", "AER2", "UER0", "UER1"];
			roadtype_flags:					bitmask(ROADTYPE_FLAG_HIDDEN);
			}
		if ((param_road_available == 0) || (param_road_available == 2)) {
			property {
			introduces_roadtype_list:		["ORD0", "ORD1", "ORD2", "ORD3", "ORD4", "SRD0"];
			speed_limit:					51 km/h;
			}
		} else
		if (param_road_available == 1) {
			property {
			introduces_roadtype_list:		["ORD0", "ORD1", "ORD2", "ORD3", "ORD4", "SRD0"];
			speed_limit:					40 km/h;
			}
		} else
		if (param_road_available == 3) {
			property {
			introduces_roadtype_list:		["ORD0", "ORD1", "ORD2", "ORD3", "ORD4", "SRD0", "SRD1", "ARD0", "IRD0"];
			speed_limit:					61 km/h;
			}
		}
			graphics {
				underlay: 				switch_ROAD_PARAM_underlay;
				bridge_surfaces: 		switch_ROAD_PARAM_bridge;
			}
		}
	}

